<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>airline</th>
      <th>airport</th>
      <th>flight#</th>
      <th>departure</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(m) { %>
      <tr>
        <td><%= m.airline %></td>
        <td><%= m.airport %></td>
        <td><%= m.flightNo %></td>
        <td><%= m.departs %></td>
        <td><a href="/flights/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
